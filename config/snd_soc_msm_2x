# CyanogenMod UseCase
SectionUseCase."Inactive" {
}
SectionUseCase."HiFi" {
SectionVerb
    Name "HiFi"

    EnableSequence
        'MI2S_RX Audio Mixer MultiMedia1':1:1
    EndSequence

    DisableSequence
        'MI2S_RX Audio Mixer MultiMedia1':1:0
    EndSequence

    # ALSA PCMs
    CapturePCM 0
    PlaybackPCM 0

EndSection
