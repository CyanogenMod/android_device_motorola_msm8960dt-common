SectionDevice
    Name "None"
EndSection
SectionDevice
    Name "Speaker"
    EnableSequence
        'SLIM RX1 MUX':0:AIF1_PB
        'SLIM_0_RX Channels':0:One
        'RX3 MIX1 INP1':0:RX1
        'RX4 DSM MUX':0:DSM_INV
        'LINEOUT1 Volume':1:50
        'LINEOUT3 Volume':1:50
        'RX3 Digital Volume':1:68
        'RX4 Digital Volume':1:68
    EndSequence

    DisableSequence
        'RX3 MIX1 INP1':0:ZERO
        'RX4 DSM MUX':0:CIC_OUT
        'LINEOUT1 Volume':1:0
        'LINEOUT3 Volume':1:0
    EndSequence

    ACDBID  15:1
    EffectsMixerCTL "SRS TruMedia"
    EC_REF_RXMixerCTL "SLIM_RX"
EndSection
